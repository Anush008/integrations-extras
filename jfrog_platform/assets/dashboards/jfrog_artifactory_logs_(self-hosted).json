                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ],
                        "queries": [
                            {
                                "compute": {
                                    "aggregation": "cardinality",
                                    "metric": "@response_content_length"
                                },
                                "data_source": "logs",
                                "group_by": [
                                    {
                                        "facet": "@repo",
                                        "limit": 10,
