                                "indexes": [
                                    "*"
                                ],
                                "name": "query1",
                                "search": {
                                    "query": "@log_source:jfrog.rt.artifactory.request @response_content_length:>=0 -@repo:\"\""
                                }
                            }
                        ],
                        "response_format": "scalar"
                    }
