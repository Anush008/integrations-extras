{
    "author_name": "Datadog",
    "description": "This dashboard provides a usage statistics overview of JFrog Artifactory logs. Included are also some high-level views of audit, request, and application logs. \n\nThe source code is also available on [Github](https://github.com/jfrog/log-analytics-datadog)",
    "layout_type": "free",
    "template_variables": [
        {
            "available_values": [],
            "default": "*",
            "name": "ip_address",
            "prefix": "@ip_address"
        },
        {
            "available_values": [],
            "default": "*",
            "name": "username",
            "prefix": "@user_name"
        },
        {
            "available_values": [],
            "default": "*",
            "name": "repo",
            "prefix": "@repo"
        }
    ],
    "title": "JFrog Artifactory Logs (Self-hosted)",
    "widgets": [
        {
            "definition": {
                "custom_links": [],
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "legend_layout": "horizontal",
                "requests": [
                    {
                        "display_type": "bars",
                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ],
                        "queries": [
                            {
                                "compute": {
                                    "aggregation": "count"
                                },
                                "data_source": "logs",
                                "group_by": [
                                    {
                                        "facet": "@repo",
                                        "limit": 10,
                                        "sort": {
                                            "aggregation": "count",
                                            "order": "desc"
                                        }
                                    }
                                ],
                                "indexes": [
                                    "*"
                                ],
                                "name": "query1",
                                "search": {
                                    "query": "@log_source:jfrog.rt.artifactory.request @repo:*?*"
                                }
                            }
                        ],
                        "response_format": "timeseries",
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "dog_classic"
                        }
                    }
                ],
                "show_legend": true,
                "title": "Accessed Repos",
                "title_align": "left",
                "title_size": "16",
                "type": "timeseries",
                "yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                }
            },
            "id": 1,
            "layout": {
                "height": 14,
                "width": 48,
                "x": 80,
                "y": 42
            }
        },
        {
            "definition": {
                "custom_links": [],
