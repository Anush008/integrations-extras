{
    "author_name": "Datadog",
    "description": "This dashboard provides usage statistics overview of JFrog Xray logs. Included are also some high level views of logs errors and volume\n\nSource code is also available in [Github](https://github.com/jfrog/log-analytics/tree/master/log-vendors/datadog)",
    "layout_type": "free",
    "template_variables": [],
    "title": "Xray Logs",
    "widgets": [
        {
            "definition": {
                "legend_columns": [
                    "avg",
                    "min",
                    "max",
                    "value",
                    "sum"
                ],
                "legend_layout": "auto",
                "markers": [],
                "requests": [
                    {
                        "display_type": "area",
                        "log_query": {
                            "compute": {
                                "aggregation": "count"
                            },
                            "group_by": [
                                {
                                    "facet": "@log_source",
                                    "limit": 10,
                                    "sort": {
                                        "aggregation": "count",
                                        "order": "desc"
                                    }
                                }
                            ],
                            "index": "*",
                            "search": {
                                "query": "@log_source:jfrog.xray.* host:xray-0"
                            }
                        },
                        "on_right_yaxis": false,
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "dog_classic"
                        }
                    }
                ],
                "show_legend": true,
                "title": "Log Volume",
                "title_align": "left",
                "title_size": "16",
                "type": "timeseries",
                "yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                }
            },
            "id": 1322847789608144,
            "layout": {
                "height": 25,
